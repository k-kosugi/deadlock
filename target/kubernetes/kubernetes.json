{
  "apiVersion" : "v1",
  "kind" : "List",
  "items" : [ {
    "apiVersion" : "v1",
    "kind" : "ServiceAccount",
    "metadata" : {
      "labels" : {
        "app" : "deadlock",
        "version" : "1.0.0-SNAPSHOT",
        "group" : "kkosugi"
      },
      "name" : "deadlock"
    }
  }, {
    "apiVersion" : "v1",
    "kind" : "Service",
    "metadata" : {
      "labels" : {
        "app" : "deadlock",
        "version" : "1.0.0-SNAPSHOT",
        "group" : "kkosugi"
      },
      "name" : "deadlock"
    },
    "spec" : {
      "ports" : [ {
        "name" : "http",
        "port" : 0,
        "targetPort" : 0
      } ],
      "selector" : {
        "app" : "deadlock",
        "version" : "1.0.0-SNAPSHOT",
        "group" : "kkosugi"
      },
      "type" : "NodePort"
    }
  }, {
    "apiVersion" : "apps/v1",
    "kind" : "Deployment",
    "metadata" : {
      "labels" : {
        "app" : "deadlock",
        "version" : "1.0.0-SNAPSHOT",
        "group" : "kkosugi"
      },
      "name" : "deadlock"
    },
    "spec" : {
      "replicas" : 1,
      "selector" : {
        "matchLabels" : {
          "app" : "deadlock",
          "version" : "1.0.0-SNAPSHOT",
          "group" : "kkosugi"
        }
      },
      "template" : {
        "metadata" : {
          "labels" : {
            "app" : "deadlock",
            "version" : "1.0.0-SNAPSHOT",
            "group" : "kkosugi"
          }
        },
        "spec" : {
          "containers" : [ {
            "env" : [ {
              "name" : "KUBERNETES_NAMESPACE",
              "valueFrom" : {
                "fieldRef" : {
                  "fieldPath" : "metadata.namespace"
                }
              }
            } ],
            "image" : "kkosugi/deadlock:1.0.0-SNAPSHOT",
            "imagePullPolicy" : "IfNotPresent",
            "livenessProbe" : {
              "failureThreshold" : 3,
              "httpGet" : {
                "path" : "/health/live",
                "port" : 0,
