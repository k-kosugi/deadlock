              },
              "initialDelaySeconds" : 0,
              "periodSeconds" : 30,
              "successThreshold" : 1,
              "timeoutSeconds" : 10
            },
            "name" : "deadlock",
            "ports" : [ {
              "containerPort" : 0,
              "name" : "http",
              "protocol" : "TCP"
            } ],
            "readinessProbe" : {
              "failureThreshold" : 3,
              "httpGet" : {
                "path" : "/health/ready",
                "port" : 0,
                "scheme" : "HTTP"
              },
              "initialDelaySeconds" : 0,
              "periodSeconds" : 30,
              "successThreshold" : 1,
              "timeoutSeconds" : 10
            }
